<h2 class='text-center'>Sign up</h2>
<div class='page'>
          <%= form_with model: @admin_users, class: 'form-box' do |form|  %><br>
            <%= form.label :first_name, class: 'control-label col-md-2' %>
            <%= form.text_field :first_name, class: 'col-md-10' %><br>
            <%= form.label :last_name, class: 'control-label col-md-2' %>
            <%= form.text_field :last_name, class: 'col-md-10' %><br>
            <%= form.label :email, class: 'control-label col-md-2' %>
            <%= form.email_field :email, class: 'col-md-10' %><br>
            <%= form.label :password, class: 'control-label col-md-2' %>
            <%= form.password_field :password, class: 'col-md-10', autocomplete: "off" %><br>
            <% if @minimum_password_length %>
              <em>(<%= @minimum_password_length %>
                                      characters minimum)</em>
            <%= form.label :email2, class: 'control-label col-md-2' %>
            <%= form.email_field :email, class: 'col-md-10' %><br>
            <%= form.label :password, class: 'control-label col-md-2' %>
            <%= form.password_field :password, class: 'col-md-10', autocomplete: "off" %><br>
            <% if @minimum_password_length %>
                                        <em>(<%= @minimum_password_length %>
                                                                characters minimum)</em>
            <%= form.label :city, class: 'control-label col-md-2' %>
            <%= form.text_field :city, class: 'col-md-10' %>
            <%= form.label :cell, class: 'control-label col-md-2' %>
            <%= form.number_field :cell, class: 'col-md-10' %>
            <%= form.label :username, class: 'control-label col-md-2' %>
            <%= form.text_field :username, class: 'col-md-10' %>
            <%= form.label :title, class: 'control-label col-md-2' %>
            <%= form.text_field :title, class: 'col-md-10', autocomplete: "off" %><br>
            <%= form.label :admin, class: 'control-label col-md-2' %>
            <%= form.check_box :admin, class: 'checkbox' %><br>
            <% for role in AdminUser::ROLES %>
             <%= check_box_tag "user[roles][#{role}]", role, @admin_users.roles.include?(role), {:name => "user[roles][]"}%>
             <%= label_tag "user_roles_#{role}", role.to_s.humanize %><br />
            <% end %>
            <%= hidden_field_tag "user[roles][]", "" %>

            <%= form.submit class: 'btn btn-success btn-lg' %>
            <% end %>

                <%= render "active_admin/shared/links" %>

          <% end %>
</div>
