<h1>Geniuses</h1>

<h1 class="text-center">
  New Project for
  <%= try(:user).try(:full_name) %>
</h1>

<div class="col-md-8">
  <table class="table table-striped">
    <thead>
      <tr>
        <% @users = User.all %>
        <% @users.each do |user| %>
        <th> Genuius </th><br/>
        <th> user  </th><br/>
      <% end %>
      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.username %></td>
          <td><%= user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
